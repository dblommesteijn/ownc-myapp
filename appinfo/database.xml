<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
	<name>*dbname*</name>
	<create>true</create>
	<overwrite>false</overwrite>
	<charset>utf8</charset>
	<table>
		<name>*dbprefix*myapp_filecache_status</name>
		<declaration>
			<field>
				<name>id</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<autoincrement>1</autoincrement>
				<length>4</length>
			</field>
			<field>
				<name>fileid</name>
				<type>integer</type>
				<notnull>true</notnull>
				<length>4</length>
			</field>
			<field>
				<name>jobid</name>
				<type>integer</type>
				<notnull>true</notnull>
				<length>4</length>
			</field>
			<field>
				<name>status</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>128</length>
			</field>
			<field>
				<name>created_at</name>
				<type>datetime</type>
			</field>
			<field>
				<name>updated_at</name>
				<type>datetime</type>
			</field>
		</declaration>
	</table>
</database>
